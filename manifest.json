{

  "description": "Simple extension letting you uncheck all checkboxes on a page.",
  "manifest_version": 2,
  "name": "unchecker",
  "version": "1.0",
  "homepage_url": "https://github.com/Ad5001/unchecker",

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icons/off.svg",
    "browser_style": true
  },

  "permissions": [
    "activeTab",
    "tabs"
  ]

}
